{
"name": "zReader",
"version": "1.3.6",
"manifest_version":2,
"icons": {
"48": "icon48.png"
},
"content_scripts": [ 
{
"js": ["base.js", "csdn.js"],
"matches": ["http://blog.csdn.net/*/article/details/*"],
"run_at": "document_end"
},
{
"js": ["base.js", "douban.js"],
"matches": ["http://www.douban.com/note/*/"],
"run_at": "document_end"
},
{
"js": ["base.js", "cnblogs.js"],
"matches": ["http://www.cnblogs.com/*/archive/*"],
"matches": ["http://www.cnblogs.com/*/p/*"],
"run_at": "document_end"
},
{
"js": ["base.js", "quora.js"],
"matches": ["http://www.quora.com/*/answer/*?srid=53KJ&share=1"],
"run_at": "document_end"
}, 
{
"js": ["base.js", "shanbay.js"],
"matches": ["http://www.shanbay.com/read/article/*/"],
"matches": ["https://www.shanbay.com/read/article/*/"],
"run_at": "document_end"
}
,{
"js":["base.js", "zhihu.js"],
"matches":["http://www.zhihu.com/question/*/answer/*"],
"matches":["https://www.zhihu.com/question/*/answer/*"],
"run_at": "document_end"
}
,{
"js":["base.js", "zhuanlan.zhihu.js"],
"matches":["http://zhuanlan.zhihu.com/p/*"],
"matches":["https://zhuanlan.zhihu.com/p/*"],
"run_at": "document_end"
}

,{
"js":["base.js", "bluereader.js"],
"matches":["http://bluereader.org/article/*"],
"run_at": "document_end"
}
,{
"js":["base.js", "xueqiu.js"],
"matches":["https://xueqiu.com/*/*"],
"run_at": "document_end"
}
]
}
